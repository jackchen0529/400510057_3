<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.calculator" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/calculator.png" />
    <icon density="mdpi" src="res/calculator.png" />
    <icon density="hdpi" src="res/calculator.png" />
    <icon density="xhdpi" src="res/calculator.png" />
    <name>calculator</name>
    <description>
        A simple calculator made by ckchia
    </description>
    <author email="jackchen0529@gmail.com" href="http://cordova.io">ckchia Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
